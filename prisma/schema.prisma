// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
    provider = "prisma-client-js"
}

datasource db {
    provider = "postgresql"
    url      = env("DATABASE_URL")
}

model Location {
    id               Int      @id @default(autoincrement())
    type             String
    BSSID            String?
    SSID             String?
    accuracy         Int?
    altitude         Int?
    battery          Int?
    batteryStatus    String?
    connection       String?
    createdAt        DateTime
    regions          String[]
    latitude         Float
    longitude        Float
    mode             Int?
    trigger          String?
    trackerId        String?
    timestamp        DateTime
    verticalAccuracy Int?
    velocity         Int?
    username         String
    device           String
}
